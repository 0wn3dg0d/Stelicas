{
  "name": "stelicas",
  "version": "1.0.1",
  "description": "Stelicas (Steam Library Categories Scraper) - A tool to scrape and organize Steam library categories.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.example.stelicas",
    "productName": "Stelicas",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "build/SCLS.ico",
      "extraFiles": [
        {
          "from": "data",
          "to": "data"
        }
      ]
    },
    "mac": {
      "target": "dmg",
      "icon": "build/SCLS.ico",
      "extraFiles": [
        {
          "from": "data",
          "to": "data"
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/SCLS.ico",
      "extraFiles": [
        {
          "from": "data",
          "to": "data"
        }
      ]
    },
    "extraResources": [
      {
        "from": "node_modules/level",
        "to": "app/node_modules/level"
      },
      {
        "from": "node_modules/level-supports",
        "to": "app/node_modules/level-supports"
      },
      {
        "from": "node_modules/level-transcoder",
        "to": "app/node_modules/level-transcoder"
      },
      {
        "from": "node_modules/abstract-level",
        "to": "app/node_modules/abstract-level"
      }
    ]
  },
  "keywords": [
    "Steam",
    "Library",
    "Categories",
    "Scraper"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "drivelist": "^10.0.0",
    "electron-store": "^8.1.0",
    "iconv-lite": "^0.6.3",
    "level": "^9.0.0",
    "level-supports": "^6.2.0",
    "level-transcoder": "^1.0.1",
    "abstract-level": "^1.0.0",
    "ps-list": "^8.1.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.6",
    "electron": "^25.0.0",
    "electron-builder": "^25.1.8"
  }
}
